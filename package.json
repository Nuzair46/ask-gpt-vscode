{
  "name": "ask-gpt-vscode",
  "displayName": "ask gpt vscode",
  "description": "chatgpt vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ask-gpt-vscode",
          "title": "Ask GPT",
          "icon": "images/logo.png"
        }
      ]
    },
    "views": {
      "ask-gpt-vscode": [
        {
          "type": "webview",
          "id": "ask-gpt.panelView",
          "name": "Ask GPT"
        },
        {
          "type": "webview",
          "id": "ask-gpt.settingsView",
          "name": "Settings"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}
